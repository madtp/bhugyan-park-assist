<?xml version='1.0' encoding='utf-8'?>
<widget id="com.app.parkassist" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Bhugyan ParkAssist</name>
    <description>
        An app based on Bhugyan GIS to help in determining closest public parking spots.
    </description>
    <icon density="hdpi" height="96" platform="android" src="res/icon/android/icon-96-xhdpi.png" width="96" />
    <author email="pgadge@tatapowersed.com" href="http://tatapowersed.com/">
        TATA Power SED Bhugyan Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-geolocation" spec="^4.0.1" />
    <plugin name="cordova-plugin-vibration" spec="^3.1.1" />
    <engine name="android" spec="^7.1.4" />
    <engine name="browser" spec="^5.0.4" />
</widget>
